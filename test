# Инициализация значений в памяти
LOAD_CONST 2       # Загружаем число 2 в регистр аккумулятора
WRITE_MEM 100      # Записываем 2 по адресу 100

LOAD_CONST 3       # Загружаем число 3
WRITE_MEM 101      # Записываем 3 по адресу 101

LOAD_CONST 4       # Загружаем число 4
WRITE_MEM 102      # Записываем 4 по адресу 102

LOAD_CONST 5       # Загружаем число 5
WRITE_MEM 103      # Записываем 5 по адресу 103

# Экспонента
LOAD_CONST 5       # Загружаем число 5
WRITE_MEM 200      # Записываем 5 в адрес 200

# Возведение в степень каждого элемента
LOAD_CONST 100     # Загружаем адрес первого элемента
READ_MEM 0         # Читаем значение из памяти
POW 200            # Возводим в степень 5
WRITE_MEM 300      # Записываем результат в адрес 300

LOAD_CONST 101     # Второй элемент
READ_MEM 0
POW 200
WRITE_MEM 301

LOAD_CONST 102     # Третий элемент
READ_MEM 0
POW 200
WRITE_MEM 302

LOAD_CONST 103     # Четвёртый элемент
READ_MEM 0
POW 200
WRITE_MEM 303
